<section class="tab stats {{tabs.stats.cssClass}}" data-group="{{tabs.stats.group}}"
  data-tab="{{tabs.stats.id}}">
  <div class="threat-contents block">
    <div class="threat-block">
      <div class="threat-section-attributes">
        <div class="section-bar attributes">
          {{localize 'torgeternity.sheetLabels.attributes'}}
          {{#if editable}}
            <a class="changeAttributesToggle" data-action="changeAttributesToggle"
              data-tooltip="{{localize 'torgeternity.iconeTitles.edit'}}">
              <i class="fas fa-edit"></i>
            </a>
          {{/if}}
          {{#if editAttributes}}
            <h5>{{localize "torgeternity.sheetLabels.alterAttributes"}}</h5>
          {{/if}}
        </div>

        <div class="threat-edit attribut-list-edit">

          {{#*inline "oneAttribute" attrName attrValue}}
            <span>
              <a
                class="skill-roll draggable" data-action="skillRoll"
                data-testType="attribute"
                data-value="{{attrValue.value}}"
                data-name="{{attrName}}"
                data-baseAttribute="{{attrName}}"
                data-adds="0"
                {{disabled (not @root.editable)}}
                data-unskilledUse="1">
                {{localize (concat 'torgeternity.attributes.' attrName)}}
              </a>
              {{#if @root.editAttributes}}
                <a class="decreaseAttribute" data-action="decreaseAttribute"
                  data-concernedAttribute="{{attrName}}">
                  <i class="fa-solid fa-minus changeAttributeSubstract"></i>
                </a>
              {{/if}}
              <input name="{{concat 'system.attributes.' attrName '.value' }}"
                type='number'
                class="attributeValueField"
                data-baseAttributeInput="{{attrName}}"
                value="{{ifThen @root.editAttributes attrValue.base attrValue.value}}"
                data-dtype='Number'
                class='small-input'
                {{#unless @root.editAttributes}} readonly="true" {{/unless}} />
              {{#if @root.editAttributes}}
                <a class="increaseAttribute" data-action="increaseAttribute"
                  data-concernedAttribute="{{attrName}}">
                  <i class="fa-solid fa-plus changeAttributeAdd"></i>
                </a>
              {{/if}}
            </span>
          {{/inline}}

          {{> oneAttribute attrName="charisma"  attrValue=document.system.attributes.charisma}}
          {{> oneAttribute attrName="dexterity" attrValue=document.system.attributes.dexterity}}
          {{> oneAttribute attrName="mind"      attrValue=document.system.attributes.mind}}
          {{> oneAttribute attrName="spirit"    attrValue=document.system.attributes.spirit}}
          {{> oneAttribute attrName="strength"  attrValue=document.system.attributes.strength}}
        </div>
      </div>
    </div>

    <div class="threat-section-actions">
      <div class="section-bar">{{localize 'torgeternity.sheetLabels.actions'}}</div>
      <div class="interaction-attack-list">
        <a
          class="unarmed-attack draggable" data-action="unarmedAttack" {{disabled (not @root.editable)}}
          data-name="unarmedCombat"
          data-base-attribute="dexterity"
          data-adds="{{document.system.skills.unarmedCombat.adds}}"
          data-skill-value="{{document.system.skills.unarmedCombat.value}}"
          data-damage="{{document.unarmed.damage}}">
          {{localize 'torgeternity.skills.unarmedCombat'}}
        </a>
        {{#unless document.system.skills.intimidation.defenseOnly}} &bull;
          <a
            class="interaction-attack draggable" data-action="interactionAttack" {{disabled (not
            @root.editable)}}
            data-name="intimidation"
            data-base-attribute="spirit"
            data-adds="{{document.system.skills.intimidation.adds}}"
            data-skill-value="{{document.system.skills.intimidation.value}}">
            {{localize 'torgeternity.skills.intimidation'}}
          </a>
        {{/unless}}
        {{#unless document.system.skills.maneuver.defenseOnly}} &bull;
          <a
            class="interaction-attack draggable" data-action="interactionAttack"
            {{disabled (not @root.editable)}}
            data-name="maneuver"
            data-base-attribute="dexterity"
            data-adds="{{document.system.skills.maneuver.adds}}"
            data-skill-value="{{document.system.skills.maneuver.value}}">
            {{localize 'torgeternity.skills.maneuver'}}
          </a>
        {{/unless}}
        {{#unless document.system.skills.taunt.defenseOnly}} &bull;
          <a
            class="interaction-attack draggable" data-action="interactionAttack"
            {{disabled (not @root.editable)}}
            data-name="taunt"
            data-base-attribute="charisma"
            data-adds="{{document.system.skills.taunt.adds}}"
            data-skill-value="{{document.system.skills.taunt.value}}">
            {{localize 'torgeternity.skills.taunt'}}
          </a>
        {{/unless}}
        {{#unless document.system.skills.trick.defenseOnly}} &bull;
          <a
            class="interaction-attack draggable" data-action="interactionAttack"
            {{disabled (not @root.editable)}}
            data-name="trick"
            data-base-attribute="mind"
            data-adds="{{document.system.skills.trick.adds}}"
            data-skill-value="{{document.system.skills.trick.value}}">
            {{localize 'torgeternity.skills.trick'}}
          </a>
        {{/unless}}
      </div>

      <div class="weapons-list">
        <div class="item-list">
          {{#each meleeweapons as |item id|}}
            <li class="item draggable" data-item-id="{{item._id}}">
              <div>
                <img src="{{item.img}}" data-tooltip="{{item.name}}" />
              </div>
              <span class="item-name {{TorgDisconnected item}}" data-action="itemName">{{item.name}}
                <span class="contradiction">{{item.contradictionCase}}</span>
              </span>
              <div class="item-controls">
                <a class="item-control item-attackRoll" data-action="itemAttackRoll" {{disabled (not @root.editable)}}
                  data-tooltip='{{localize "torgeternity.iconeTitles.attackRoll"}}'>
                  <i class="fas fa-dice-d20"></i>
                </a>
                <a class="item-control item-edit" data-action="itemEdit"
                  data-tooltip='{{localize "torgeternity.iconeTitles.edit"}}'>
                  <i class="fas fa-edit"></i>
                </a>
                <a class="item-control item-delete" data-action="itemDelete" {{disabled (not @root.editable)}}
                  data-tooltip='{{localize "torgeternity.iconeTitles.delete"}}'>
                  <i class="fas fa-trash"></i>
                </a>
              </div>
            </li>
          {{/each}} {{#each missileweapon as |item id|}}
            <li class="item draggable" data-item-id="{{item._id}}">
              <div>
                <img src="{{item.img}}" data-tooltip="{{item.name}}" />
              </div>
              <span class="item-name {{TorgDisconnected item}}" data-action="itemName">{{item.name}}
                <span class="contradiction">{{item.contradictionCase}}</span>
              </span>
              <div class="item-controls">
                <a
                  class="item-control item-attackRoll" data-action="itemAttackRoll" {{disabled (not @root.editable)}}
                  data-tooltip='{{localize "torgeternity.iconeTitles.attackRoll"}}'>
                  <i class="fas fa-dice-d20"></i>
                </a>
                {{#if (not ../ignoreAmmo)}}
                  <a class="item-control" data-weapon="{{item}}" {{disabled (not @root.editable)}}
                    data-tooltip="{{localize 'torgeternity.iconeTitles.reload'}}">
                    <i class="fa-solid fa-bullseye reload-weapon" data-action="reloadWeapon"></i>
                  </a>
