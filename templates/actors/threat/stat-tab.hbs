                <a class="item-control item-edit" data-action="itemEdit"
                  data-tooltip='{{localize "torgeternity.iconeTitles.edit"}}'>
                  <i class="fas fa-edit"></i>
                </a>
                <a
                  class="item-control item-delete" data-action="itemDelete" {{disabled (not @root.editable)}}
                  data-tooltip='{{localize "torgeternity.iconeTitles.delete"}}'>
                  <i class="fas fa-trash"></i>
                </a>
              </div>
            </li>
          {{/each}} {{#each psionicpower as |item id|}}
            <li class="item draggable" data-item-id="{{item._id}}">
              <div>
                <img src="{{item.img}}" data-tooltip="{{item.name}}" />
              </div>
              <span class="item-name {{TorgDisconnected item}}" data-action="itemName">{{item.name}}
                <span class="contradiction">{{item.contradictionCase}}</span>
              </span>
              <div class="item-controls">
                <a
                  class="item-control item-powerRoll" data-action="itemPowerRoll" {{disabled (not @root.editable)}}
                  data-tooltip='{{localize "torgeternity.iconeTitles.psionicRoll"}}'>
                  <i class="fas fa-dice-d20"></i>
                </a>
                <a class="item-control item-edit" data-action="itemEdit"
                  data-tooltip='{{localize "torgeternity.iconeTitles.edit"}}'>
                  <i class="fas fa-edit"></i>
