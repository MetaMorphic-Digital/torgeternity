<div class="effectsPanel">
  {{#each effects}}
    {{#if active}}
      <div class="effect">
        <img src="{{img}}" data-tooltip="{{name}}{{#if isTemporary}} &#x1F551;{{/if}}"
          data-tooltip-direction="LEFT" data-action="clickEffect" data-uuid="{{uuid}}" />
      </div>
    {{/if}}
  {{/each}}
</div>